{
  "name": "@mergin/lib",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "main": "dist/lib.js",
  "module": "dist/lib.js",
  "types": "types/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.js"
    },
    "./dist/": {
      "import": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build:watch": "vue-demi-switch 3 && npm-run-all -p build:lib:watch build:types:watch",
    "build:lib": "vue-demi-switch 3 && yarn clean:types:temp && vite build --mode development",
    "build:lib:prod": "vue-demi-switch 3 && yarn clean:types:temp && vite build",
    "build:lib:watch": "vue-demi-switch 3 &&  yarn clean:types:temp && vite build --watch --mode development",
    "build:lib:all": "yarn build:lib && yarn build:types",
    "build:lib:all:prod": "vue-demi-switch 3 && yarn build:lib:prod",
    "build:types": "yarn clean:types && (vue-tsc --declaration --emitDeclarationOnly || true) && tsc-alias",
    "build:types:watch": "yarn build:types && (concurrently \"vue-tsc --watch\" \"tsc-alias -w\")",
    "clean": "yarn clean:lib && yarn clean:types",
    "clean:lib": "rimraf ./dist",
    "clean:types:temp": "rimraf ./**/*.vue.ts",
    "clean:types": "rimraf ./types ./tsconfig.tsbuildinfo && yarn clean:types:temp",
    "link:register": "yarn link",
    "link:unregister": "yarn unlink || true",
    "lint": "eslint . --ext .js,.vue,.ts"
  },
  "dependencies": {
    "axios": "^0.28.1",
    "axios-retry": "^3.2.4",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "primeflex": "^3.3.1",
    "primevue": "^3.43.0",
    "vue": "^3.3.8",
    "vue-router": "4.2.5"
  },
  "peerDependencies": {
    "pinia": ">=2.x",
    "portal-vue": "3.x",
    "vue": "3.x"
  },
  "devDependencies": {}
}
